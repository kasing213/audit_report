  "scripts": {
    "build": "tsc",
    "prestart": "npm run build",
    "start": "node dist/index.js",
    "dev": "ts-node src/index.ts",
    "watch": "ts-node --watch src/index.ts",
    "lint": "eslint src/**/*.ts",
    "typecheck": "tsc --noEmit"
