    "handlebars": "^4.7.8",
    "mongodb": "^6.3.0",
    "node-cron": "^3.0.3",
    "puppeteer": "^21.11.0",
    "telegraf": "^4.15.7"
  },
  "devDependencies": {
    "@types/express": "^4.17.21",
    "@types/node": "^20.10.5",
    "@typescript-eslint/eslint-plugin": "^6.15.0",
    "@typescript-eslint/parser": "^6.15.0",
    "eslint": "^8.56.0",
    "ts-node": "^10.9.2",
    "typescript": "^5.3.3"
  }
